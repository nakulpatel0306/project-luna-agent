# luna backend environment variables

# api keys (optional - for cloud fallback)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# llm configuration
LLM_PROVIDER=ollama  # ollama, openai, anthropic
LLM_MODEL=llama3
LLM_TEMPERATURE=0.7

# database
DATABASE_URL=sqlite:///luna.db

# api configuration
API_HOST=127.0.0.1
API_PORT=8000

# development
DEBUG=true
LOG_LEVEL=info
